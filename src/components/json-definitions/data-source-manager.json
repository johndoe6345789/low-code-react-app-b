                        "label": {
                          "source": "data.source.groups.kv"
                        },
                        "onDelete": {
                          "source": "data.handleDeleteSource"
                        },
                        "onEdit": {
                          "source": "data.handleEditSource"
                        }
                      },
                      "type": "DataSourceGroupSection"
                    },
                    {
                      "props": {
                        "dataSources": {
                          "source": "groupedSources.static"
                        },
                        "getDependents": {
                          "source": "data.getDependents"
                        },
                        "icon": {
                          "source": "data.icon.FileText"
